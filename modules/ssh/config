Include config.d/*

Host github.com
    # We pre-load the host keys for github.com, so check them strictly
    StrictHostKeyChecking Yes

Host shell.gh
    Hostname shell.service.cp1-iad.github.net
    ForwardAgent yes
    ProxyJump bastion.githubapp.com

Host *.github.net
    ForwardAgent yes
    ProxyJump bastion.githubapp.com