// Place your settings in this file to overwrite the default settings
{
    "[c]": {
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        },
        "editor.defaultFormatter": "xaver.clang-format"
    },
    "[cpp]": {
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "[json]": {
        "editor.formatOnSave": false,
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "C_Cpp.loggingLevel": "Debug",
    "csharp.suppressDotnetRestoreNotification": true,
    "debug.allowBreakpointsEverywhere": true,
    "editor.find.addExtraSpaceOnTop": false,
    "editor.fontFamily": "'Victor Mono', 'Delugia Nerd Font', 'Cascadia Code', Consolas, 'Courier New', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "editor.minimap.enabled": false,
    "editor.suggestSelection": "first",
    "editor.wordBasedSuggestions": false,
    "editor.wordWrap": "off",
    "terminal.integrated.fontFamily": "'Victor Mono', 'Cascadia Code', Consolas, 'Courier New', monospace",
    "vim.leader": " ",
    "workbench.iconTheme": "vs-seti",
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "go.delveConfig": {
        "dlvLoadConfig": {
            "followPointers": true,
            "maxVariableRecurse": 1,
            "maxStringLen": 4096,
            "maxArrayValues": 128,
            "maxStructFields": -1
        },
        "apiVersion": 2,
        "showGlobalVariables": false
    },
    "vs-kubernetes": {
        "disable-lint": true
    },
    "editor.renderWhitespace": "boundary",
    "ruby.useLanguageServer": true,
    "ruby.format": "rubocop",
    "terminal.external.osxExec": "iTerm.app",
    "git.enableSmartCommit": true,
    "workbench.colorTheme": "SynthWave '84",
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": ["storage", "keyword", "meta.macro.rules.rust", "meta.tag.table.toml"],
                "settings": {
                    "fontStyle": "italic bold"
                }
            },
            {
                "scope": ["keyword.operator", "punctuation"],
                "settings": {
                    "fontStyle":  "bold"
                }
            },
            {
                "scope": ["meta.function.definition", "meta.attribute", "keyword.key.toml"],
                "settings": {
                    "fontStyle": "italic"
                }
            },
            {
                "scope": ["meta.interpolation"],
                "settings": {
                    "fontStyle": "bold",
                    "foreground": "#FEDE5D"
                }
            }
        ]
    },
    "workbench.colorCustomizations": {

        "terminal.ansiRed": "#CB2431",
        "terminal.ansiBrightBlack": "#41474E",
        "terminal.ansicyan": "#5A32A3",
        "terminal.ansiWhite": "#D1D5DA",
        "terminal.ansiBrightCyan": "#B392F0",
        "terminal.ansiBlue": "#005CC5",
        "terminal.ansiBrightYellow": "#FFEA7F",
        "terminal.ansiYellow": "#F9C513",
        "terminal.ansiGreen": "#22863A",
        "terminal.ansiBrightGreen": "#85E89D",
        "terminal.ansiBrightMagenta": "#F692CE",
        "terminal.ansiBrightBlue": "#79B8FF",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "terminal.ansiBlack": "#24292E",
        "terminal.background": "#24292E",
        "terminal.ansiBrightRed": "#F97583",
        "terminal.foreground": "#FAFBFC",
        "terminal.ansiMagenta": "#D03592",
        "editorInlayHint.foreground": "#717171"
    },
    "powershell.integratedConsole.showOnStartup": false,
    "sde.languageServerMode": "sourcekit-lsp",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "window.newWindowDimensions": "offset",
    "foam.openDailyNote.directory": "daily",
    "foam.openDailyNote.filenameFormat": "yyyy-mm-dd",
    "foam.openDailyNote.titleFormat": "'Daily notes for' dddd, mmmm d, yyyy",
    "terminal.integrated.automationShell.linux": "/bin/zsh",
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "terminal.integrated.fontWeightBold": "500",
    "java.configuration.checkProjectSettingsExclusions": false,
    "[yaml]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "gitlens.codeLens.scopes": [
            "document"
        ],
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": true
        }
    },
    "liveshare.authenticationProvider": "GitHub",
    "go.toolsManagement.autoUpdate": true,
    "[ruby]": {
        "editor.defaultFormatter": "misogi.ruby-rubocop"
    },
    "command-runner.commands": {
    },
    "[typescript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter.notebook.ipynb"
    },
    "editor.inlineSuggest.enabled": true,
    "github.copilot.enable": {
        "*": true,
        "yaml": false,
        "plaintext": false,
        "markdown": false
    },
    "redhat.telemetry.enabled": true,
    "omnisharp.path": "latest",
    "terminal.integrated.defaultProfile.linux": "zsh",
    "files.associations": {
        "*.asm": "asm-intel-x86-generic"
    },
    "sourcekit-lsp.serverPath": "/Library/Developer/CommandLineTools/usr/bin/sourcekit-lsp",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "vscode-edge-devtools.mirrorEdits": true,
    "[python]": {
    }
}