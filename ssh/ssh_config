Host bastion
  HostName bastion.githubapp.com
  ForwardAgent yes
  ControlMaster auto
  ControlPath ~/.ssh/sockets/%C
  ControlPersist 600
Host shell
  HostName shell.service.cp1-iad.github.net
  ForwardAgent yes
  ProxyJump bastion.githubapp.com
Host *.github.net
  ForwardAgent yes
  ProxyJump bastion.githubapp.com
